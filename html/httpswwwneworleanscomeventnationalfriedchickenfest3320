<script>
	
		require(["jquery"], function( $) {
			$('title').html("Fried Chicken Fest | New Orleans");
		});
	

	require([
		"jquery",
		"sv_goatee!plugins=stringLib,clientMoment,sv_site,cloudinary",
		"site_gtm",
		"plugins_listings_custom_map",
		"geodist",
		"plugins_listings_custom_snazzymaps",
		"sv_load!site_tripbuilder",
		"sv_siteFoundation"
	], function(
		$,
		goatee,
		gtm,
		mapObj,
		geodist,
		snazzymaps,
		tripbuilder
	) {
		var root = $("#eventDetails_655f2fb0-dfb8-4f18-9bc2-54e400587f10");
		var sharedDetail = root.find('.shared-detail');

