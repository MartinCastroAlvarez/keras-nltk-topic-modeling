		<div class="contentRender contentRender_16 contentRender_type_widget contentRender_name_plugins_events_detail" data-sv-time="40" data-sv-index="16" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>










<div id="eventDetails_655f2fb0-dfb8-4f18-9bc2-54e400587f10" class="listings" data-type="event" data-recid="3320" data-sv-detail-page data-gtm-vars='{"tClient": {"eventCategory": "Events", "eventLabel": "Woldenberg%20Park | National%20Fried%20Chicken%20Fest", "et": "National%20Fried%20Chicken%20Fest", "vt": "Detail", "ua": "Click"}}'>
	<script type="text/template" data-sv-infoWindowTemplate><div class="infoWindow myDiv">
	{{!listings}}
		<h5>{{title}}</h5>
		<ul class="contactInfo no-bullet">
			{{:address1}}
				<li>{{address1}}</li>
			{{/}}
			{{:city}}
				<li>{{~plugins.stringLib.formatCityStateZip({ city : data.city, state : data.state, zip : data.zip })}}</li>
