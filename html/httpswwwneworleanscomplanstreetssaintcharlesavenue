<script>
	require(["jquery"], function($) {
		// This should exist in the header panel and adds the body class as quickly as possible to hide
		// all the items on the page so we don't have the site flash before the calendar shows up
		if ($('.body-section').find('.interior-nav').length !==0){
			$('.body-section').addClass('no-top-margin');
		} else if ($('.body-section').find('.shared-detail').length !==0) {
			$('.body-section').addClass('detail-page');
		} else {
			if($('.body-section').find('.blogContainer').length < 1) {
				$('.body-section').addClass('has-top-margin');
			}
		};
	})
</script>
