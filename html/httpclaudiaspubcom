  if (navigator.userAgent.match(/iPhone/i)
     || navigator.userAgent.match(/iPad/i)
     || navigator.userAgent.match(/iPod/i)){
    $(document).ready(function(){
          document.body.className += " " + "webkit-mobile-body";
    });
  }

  var promotion_url_param_domain = getPromotionUrlParams()['domain'];
  if ( promotion_url_param_domain && ( new URL(document.location).host == 'localhost:4000' || new URL(document.location).host == 'wcache.spotapps.co')){
    $("a[href^='/']").each(function(){
      if (this.href.indexOf('domain=') == -1){
        var sh_internal_url = new URL(this.href);
        sh_internal_url.searchParams.set("domain", promotion_url_param_domain);
        this.href = sh_internal_url.toString();
      }
    })
  }


  function cta_link_matches_href(href, url){
    var domain = 'claudiaspub.com';
    var partsOfURL = url.split(domain);
    if (partsOfURL.length != 2){
      return href == url;
    }else{
      var urlPath = partsOfURL[1];
      if (urlPath.substring(0,1) == "/"){
        urlPath = urlPath.substring(1,urlPath.length);
      }
      return href.indexOf(urlPath, href.length - urlPath.length) !== -1;
    }
  }

  function find_cta_link(href) {
    var all_cta_links = [{"id":260262,"spot_id":56157,"url":"https://claudiaspub.com/-food-menu","cta_link_type":"food_menu","created_at":"2019-07-25T19:48:30+00:00","updated_at":null},{"id":260263,"spot_id":56157,"url":"https://claudiaspub.com/-drink-menu","cta_link_type":"drink_menu","created_at":"2019-07-25T19:48:30+00:00","updated_at":null},{"id":260264,"spot_id":56157,"url":"https://claudiaspub.com/-events","cta_link_type":"events","created_at":"2019-07-25T19:48:30+00:00","updated_at":null},{"id":260265,"spot_id":56157,"url":"https://claudiaspub.com/-gift-cards","cta_link_type":"gift_cards","created_at":"2019-07-25T19:48:30+00:00","updated_at":null}];

    for (var i=0; i< all_cta_links.length; i++){
      var cta_link = all_cta_links[i];
      if ( cta_link_matches_href(href, cta_link.url)){
        return cta_link;
      }
    }
  }

  function element_has_events_defined(elem){
    if ($._data(elem, "events") === undefined){
      return false;
    }
    return true;
  }

  function create_ga_events_for_cta_clicks(){
    if (typeof $ === 'function'){

      var cta_link_of_the_page = find_cta_link(location.href);


      $("a").each(function(index, elem){
        var href = $(elem).attr('href');

        if (href  === undefined ){
           return;
        }

        if (href.substring(0,4) == 'tel:'){
          if (!element_has_events_defined(elem)){  // not footer links
            $(elem).on("click", function(){
                ga('send', 'event', {
                  eventCategory: 'Button',
                  eventAction: 'Click',
                  eventLabel: 'Phone',
                  transport: 'beacon'
                });
             });

             if (cta_link_of_the_page){
                $(elem).on("click", function(){
                    ga('send', 'event', {
                      eventCategory: 'Button',
                      eventAction: 'Click',
                      eventLabel: cta_link_of_the_page.cta_link_type +  ' page phone',
                      transport: 'beacon'
                    });
                 });
             }
          } else {
             if (cta_link_of_the_page){
               $(elem).on("click", function(){
                   ga('send', 'event', {
                        eventCategory: 'Button',
                        eventAction: 'Click',
                        eventLabel: cta_link_of_the_page.cta_link_type +  ' page phone footer',
                        transport: 'beacon'
                   });
               });
             }
          }


        }else{
          var cta_link = find_cta_link(href);
          if (cta_link != null){
            $(elem).on("click", function(){
              ga('send', 'event', {
                      eventCategory: 'Website Link',
                      eventAction: 'Click',
                      eventLabel: cta_link.cta_link_type,
                      transport: 'beacon'
                });
            });
          }
        }


      });
    }
  }

  if (typeof angular === "undefined") {
    create_ga_events_for_cta_clicks();
  } else {
    angular.element(document).ready(function () {
      create_ga_events_for_cta_clicks();
    });
  }


  function triggerShowFooterEvents(){
    if (window.screen.availWidth < 768){
        ga('send', 'event', {
                eventCategory: 'Mobile Footer',
                eventAction: 'View',
                eventLabel: '',
                transport: 'beacon'
          });
    }
  }

</script></body></html>