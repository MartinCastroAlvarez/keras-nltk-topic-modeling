    function init_open_street_map() {
      var map = L.map('map-v3-container', {
          center: [ 45.5119706, -122.634553],
          zoom: 16,
          zoomControl: false,
          dragging:false
      });

      if (marker_icon.length>0){
        var myIcon = L.icon({
            iconUrl: marker_icon,
            iconSize: [35,35]
        });
        var marker = L.marker([45.5119706, -122.634553], {icon: myIcon}, {title: '3006 Southeast Hawthorne Boulevard, Portland'}, {alt: 'map pin icon'}).addTo(map);
      } else {
        var marker = L.marker([45.5119706, -122.634553], {title: '3006 Southeast Hawthorne Boulevard, Portland'}, {alt: 'map pin icon'}).addTo(map);
      }

  		marker.on('click', open_google_map);

      L.tileLayer.provider('Wikimedia').addTo(map);
      L.control.zoom({
       position:'bottomright'
      }).addTo(map);

      map.scrollWheelZoom.disable();
    }
