<script>
	require([
		"jquery",
		"sv_goatee!plugins=stringLib,crmLib,sv_site,cloudinary,miscLib",
		"sv_crmLib",
		"site_gtm",
		"plugins_listings_custom_listings_lib",
		"plugins_listings_custom_map",
		"plugins_listings_custom_snazzymaps",
		"plugins_listings_custom_responsive_tables",
		"geodist",
		"sv_arrayLib",
		"plugins_core/main",
		"sv_asyncLib",
		"sv_load!site_tripbuilder",
		"sv_cloudinaryLib",
		"plugins_listings_custom_rates",
		"plugins_common_custom_magnific",
		"sv_siteFoundation"
	], function(
		$,
		goatee,
		sv_crmLib,
		gtm,
		listingsLib,
		mapObj,
		snazzymaps,
		responsive_tables,
		geodist,
		arrayLib,
		core,
		asyncLib,
		tripbuilder,
		sv_cloudinaryLib,
		rates

	) {
		var root = $("#listingDetail_f71f5433-66fb-4559-b2ce-7b74c6111183");
		var infoWindowTmpl = root.find("[data-sv-infoWindowTemplate]").html();
		var affiliateTemplate = root.find('[data-sv-affiliateTemplate]').html();
		var coolCousinTemplate = root.find('[data-sv-coolCousinTemplate]').html();
		var liItemsTemplate = root.find("[data-sv-liItems]").html();
		var tripadvisorTemplate = root.find("[data-sv-tripadvisorTemplate]").html();
		var sharedDetail = root.find('.shared-detail')

		var map_points = [
			convention_center = {
				lat: 29.939564,
				long: -90.063285
			},
			superdome_arena = {
				lat: 29.951060,
				long: -90.081197
			},
			airport = {
				lat: 29.984492,
				long: -90.255986
			},
			french_quarter = {
				lat: 29.957443,
				long: -90.062935
			}
		];

		// only stringify the fields accessed in front-end JS
		var data = {"recid":32817,"regionid":1,"region":"Central Business District/Downtown","crmtracking":{"core_mobile_call":"18_32817","core_itinerary":"58_32817","core_facebook_click":"15_32817","core_booking_click":"7_32817","core_map_view":"59_32817","core_mobile_click":"16_32817","core_listing_view":"1_32817","core_listing_click":"4_32817","core_twitter_view":"12_32817","core_mobile_view":"17_32817","core_facebook_view":"14_32817","core_twitter_click":"13_32817"},"thisListing":"5d25207d869088df9c467894","placemark":{"recid":32817,"title":"Lafayette Square","latitude":29.948034,"longitude":-90.070419,"address1":"602 Camp St.","city":"New Orleans","state":"LA","zip":"70130","nearbyRecid":32817}};



		//trip builder
		if (tripbuilder !== undefined) {
			tripbuilder.initButtons({
				root : (root.parent().hasClass('qv-content')) ? $('[data-qv-wrapper]') : root, // check whether we're inside qv
				addButtonTemplate : '<div class="trip-add"><a data-tb-add data-gtm-click=\'{ "collect" : true }\' data-gtm-vars=\'{ "tCore" : { "action" : "_trip_builder_add" }, "tClient" : { "eventAction" : "TripBuilder - Add", "lt" : "TripBuilder", "ui" : "Conversion" } }\' ><i class="fa fa-heart-o"></i></a></div>',
removeButtonTemplate : '<div class="trip-add remove"><a data-tb-remove data-gtm-click=\'{ "collect" : true }\' data-gtm-vars=\'{ "tCore" : { "action" : "_trip_builder_remove" }, "tClient" : { "eventAction" : "TripBuilder - Remove", "lt" : "TripBuilder" } }\'><i class="fa fa-heart"></i></a></div>'

			});

			tripbuilder.events.on("updateButton", function() {
				gtm.update();
			});
		}

		root.find("[data-sv-venuespecs]").on('click', function() {
			$(this).toggleClass('active');
			$(this).parent().toggleClass('active');
		});

		root.find('[data-sv-open-popup]').magnificPopup({
			type:'inline',
			midClick: true
		});

		// validate and track social urls
		root.find("[data-sv-social]").each(function() {
			var validServices =['facebook', 'twitter', 'youtube', 'instagram', 'pinterest'];
			var service = $(this).data('sv-service').toLowerCase();
			if (validServices.indexOf(service) === -1) { return; }

			var href = listingsLib.getValidSocialUrl({ url : $(this).attr('href'), urlType : service });
			switch (service) {
				case "twitter":
					$(this).attr("href", sv_crmLib.getTrackUrl(data.crmtracking.core_twitter_click, href));
					break;
				case "facebook":
					$(this).attr("href", sv_crmLib.getTrackUrl(data.crmtracking.core_facebook_click, href));
					break;
				default:
					$(this).attr("href", href);
			}
		});

		/**** CRM tracking - DO NOT REMOVE!!!! ****/
		sv_crmLib.trackAjax(data.crmtracking.core_listing_view);

		

		
			var taArgs = { service : "tripadvisor", recid : data.recid };
			$.get("/includes/plugins/listings/getrenderedreviews/", taArgs, function(res) {
				if (res.success) {
					root.find("[data-sv-tripadvisor-pane]").html(res.html);
				}
			})
		

		if(data.coolcousinid !== undefined){
			var spot_id = data.coolcousinid;

			$.get("https://api.coolcousin.com/spots_inline_cousins/"+spot_id, function(res) {
				if (res.recommendation_set) {
					root.find(".review-tab-title.inactive").removeClass("inactive")

					var coolcousinHtml = goatee.fill(coolCousinTemplate, {data : res });
					root.find("[data-sv-coolcousin-pane]").html(coolcousinHtml);
				}
			});

		}

		if(data.regionid !== undefined){
			var xhr1;
			var queryFields = {
				region : 1,
				link_raw : 1
			}
			var flowRegion = new asyncLib.Flow();
			flowRegion.series({
				filter : function(cb) {
					var filter = {
						region : data.regionid.toString()
					};

					cb(null, filter);
				},
				regionData : function(cb) {
					var options = {
						castDocs : false,
						fields : queryFields,
					};

					if (xhr1) { xhr.abort(); }

					xhr1 = $.get("/includes/rest_v2/plugins_collections_type_custom_region_slides/find/", { json : JSON.stringify({ filter : flowRegion.data.filter, options : options }), token : core.simpleToken });

					xhr1.done(function(res) {
						cb(null, res.docs[0]);
					}).fail(function(err, type, message) {
						if (type === "abort") { return flow.halt(cb); } // another request has aborted this one, so halt this flow
						return cb(new Error(message));
					});
				},
				render : function(cb) {
					if(flowRegion.data.regionData === undefined || flowRegion.data.regionData.region === undefined){ return cb(null); }

					var update = '<a href="' + flowRegion.data.regionData.link.url + '" target="'+ flowRegion.data.regionData.link.target + '">' + data.region + '</a>'

					root.find("[data-sv-region-update]").html(update);
					cb(null);
				},
				display : function(cb) {
					root.find("[data-sv-region-display]").css({"visibility" : "visible"});
				}
			}, function(err) {
				if (err) { throw err; }
			});
		}
		
		var xhr2;
		var queryFields = {
			description : 1,
			detailURL : 1,
			dtn : 1,
			isDTN : 1,
			media : 1,
			primary_site : 1,
			primary_image : 1,
			primary_image_url : 1,
			rankid : 1,
			rankorder : 1,
			rankname : 1,
			recid : 1,
			regionid : 1,
			state : 1,
			title : 1,
			detail_type : 1,
			qualityScore : 1,
			hasTripAdvisor: 1,
			taid : 1,
			taoptin : 1,
			"accountudfs_object.766" : 1,
			"amenities.restaurant_pricerange.value" : 1
		}

		var coolcousin;

		var flow = new asyncLib.Flow();
		flow.series({
			filter : function(cb) {
				var filter = {
					"listingudfs_object.798.value_raw" : data.recid,
					"accountudfs_object.766.value" : {"$ne" : "Permanently Closed"}
				};

				if (data.thisListing !== undefined){
					filter._id = {"$ne" : { "$oid" : data.thisListing}}
				}

				cb(null, filter);
			},
			data : function(cb) {
				var options = {
					castDocs : false,
					fields : queryFields,
					count : true,
					sort : {
						rankorder : 1,
						sortcompany : 1
					},
				};

				if (xhr2) { xhr.abort(); }

				xhr2 = $.get("/includes/rest_v2/plugins_listings_listings/find/", { json : JSON.stringify({ filter : flow.data.filter, options : options }), token : core.simpleToken });

				xhr2.done(function(res) {
					var count;
					var data;

					count = res.docs.count;
					
					if(count == 0){
						root.find("[data-sv-detailtoggle]").on('click', function() {
							$(this).toggleClass('active');
							$(this).parent().toggleClass('active');
						});
						return;
					}
					
					data = res.docs.docs;

					cb(null, { data : data, count : count });
				}).fail(function(err, type, message) {
					if (type === "abort") { return flow.halt(cb); } // another request has aborted this one, so halt this flow
					return cb(new Error(message));
				});
			},
			render : function(cb) {

				flow.data.data.data.forEach(function(val){
					if (val.primary_image_url !== undefined) {
						val.primary_image = { resource : sv_cloudinaryLib.default.createResource({ imageUrl : val.primary_image_url }) };
					}
				});

				var affiliateHtml = goatee.fill(affiliateTemplate, {data : flow.data.data.data }, {custom_li_items : liItemsTemplate});
				root.find("[data-sv-affiliated-items]").html(affiliateHtml);


				cb(null);
			},
			updateTripbuilder : function(cb){
				// tripbuilder
				if (tripbuilder !== undefined) {
					tripbuilder.updateButtons(root);
				}

				cb(null);
			},
			clickFunction : function(cb){
				root.find("[data-sv-detailtoggle]").on('click', function() {
					$(this).toggleClass('active');
					$(this).parent().toggleClass('active');
				});

				cb(null);
			},
			getTripAdvisor : function(cb) {
				$.each(flow.data.data.data, function (index, listing) {
					if(listing.hasTripAdvisor === true){
						var taArgs = { service : "tripadvisor", recid : listing.recid};
						$.get("/includes/plugins/listings/getreviews/", taArgs, function(res) {
							if (res.success) {
								var listingItem = root.find(".item[data-recid='"+ listing.recid + "']");
								var taTemplate = listingItem.find('[data-sv-tripadvisor-item]');
								taTemplate.html(goatee.fill(tripadvisorTemplate, res));
							}
						});
					}
				});
				cb(null);
			}
		}, function(err) {
			if (err) { throw err; }
		});
		
		var placemarks = [{
			id : data.placemark.recid,
			marker : {
				position : [data.placemark.latitude, data.placemark.longitude]
			},
			infoWindow : {
				content : goatee.fill(infoWindowTmpl, data.placemark),
				maxWidth: 253
			}
		}];

		// begin: Google Maps logic
		root.on("click", ".map-toggle [data-openmap]", function(e) {
			e.preventDefault();
			mapObj.init({
				node : root,
				map : {
					markerIcon : '/includes/public/assets/images/listings/list-pin.png',
					mapOptions : {
						zoom: 16,
						styles : snazzymaps.nola
					}
				}
			}, true);
			mapObj.setState({
				isQuickview: false,
				open : true,
			});
			mapObj.setPlacemarks(placemarks);
			sharedDetail.toggleClass('map-open');
		});

		root.on("click", ".map-toggle [data-closeMap]", function(e) {
			e.preventDefault();
			mapObj.setState({open : false});
			sharedDetail.toggleClass('map-open');
		});

		var distances = map_points.map(function(val) {
			return new geodist({
				start : { lat : val.lat, lon : val.long },
				end : { lat : data.placemark.latitude, lon : data.placemark.longitude }
			}).format({ unit : "miles" });
		});

		var distancesCounter = 0;
		distances.forEach(function(point){
			distancesCounter ++
			root.find('[data-sv-map-points] .miles'+distancesCounter).html(point + " MILES");
		});

		// foundation tab init
		$(document).foundation("tab", "reflow");
		root.find(".left-nav.tabs.no-stack").show().find("> dd:first > a").click();

		// begin: amenities tab subtabs
		root.find(".tabs.subtab > dd > a").on("click", function(e) {
			e.preventDefault();

			var tid = $(this).attr('href');
			var t = $(this).closest('.tabs');
			var dd = $(this).closest('dd');
			var tc = t.closest('.tabs-content').closest('.content');
			tc.removeClass('active');
			$(tid).addClass('active');
			$(tid).siblings().removeClass('active');
			dd.addClass('active');
			dd.siblings().removeClass('active');
			t.toggleClass('open');
		});

		root.find(".tabs.subtab").each(function(i) {
			if ($(this).find('dd').length < 2) {
				$(this).addClass('solo');
			}
		});

		$(window).on("resize", function() {
			/*  when using open left menu tabs in desktop and closed dropdown style tabs
				in mobile we want to reset the open state. otherwise clicking on a tab in
				desktop will leave it stuck on open when re-sizing to responsive */
			$(".listings .tabs.subtab.open").removeClass("open");
		});

		/*** BEGIN Rates ***/

			var rateTmpl = '<div class="rate-wrapper"><span class="rates">Avg Rate Per Night </span><span class="price"><strong>${{lowrate}}</strong></span></div>';
			var linkTmpl = '<div class="booking"><a href="{{bookurl}}" target="_blank" class="bookLink button basic">Book Now</a></div>';

			var addRates = function() {
				rates.populateSummary({node: root, rateTmpl: rateTmpl, linkTmpl: "", cleanup: true });
			}
			addRates();
		/*** END Rates ***/
		// end: amenities tab subtabs
	});
</script></div><div class="contentRender contentRender_17 contentRender_type_widget contentRender_name_plugins_addthis_addthis" data-sv-time="1" data-sv-index="17" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'><div class="share-top-right page-atn  addthis atn-share addthis_toolbox addthis_default_style" data-guid="f0265644-9eb1-4ff7-a4bf-dde0ef206088">
