	<sv_script>
			require(["jquery", "sv_clientMoment"], function($, clientMoment) {
	
				var days = {{~stringify(data.value.days || "")}};
				var info = {{~stringify(data.value.details || "")}};
				
				days.forEach(function(time){
					if(clientMoment().format("ddd").toLowerCase() === time.code.substring(0,3)){
						if(time.open != undefined && time.close != undefined){
							$('[data-sv-active-time]').html(time.label +' '+ time.open +'-'+ time.close);
						}
						$('.hours_dropdown').css({'opacity' : 1});
					}     	
				});
		
			});
	</sv_script>
