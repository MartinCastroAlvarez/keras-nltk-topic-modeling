						    var paged = '2';
						    var posts_per_page = '15';
						    var post_type = 'product';
						    var all_loaded = false;
						    var ajax_running = false;

						    jQuery(function(){

						    	jQuery(window).scroll(function() {
								    if (jQuery(window).scrollTop() == jQuery(document).height() - jQuery(window).height()) {								    
								     	if (!all_loaded && !ajax_running) {
								     		ajax_running = true;
								     		jQuery('.grid-row').after('<div class="loader">Loading...</div>');
								     		var loaddata = {action: "load_home_posts", paged: paged, posts_per_page: posts_per_page, post_type: post_type};
								     		console.log(loaddata);
										    jQuery.ajax({
											   url: '/wp-admin/admin-ajax.php',
											   type: 'POST',
											   data : loaddata,
											   error: function() {
											      console.log('An error has occurred.');
											   },
											   success: function(data) {
											   	var length = jQuery(data).find('.grid-col').andSelf().length; //count html objects
											   	//console.log(length);
											   	jQuery('.loader').remove();
									    	    jQuery(".grid-row").append(data).ready(function(){
									    	    	window.sr = ScrollReveal();
									    	    	console.log('.gc'+paged);
													sr.reveal('.gc'+paged,{ duration: 1 }, 150);
									    	    });									    	    
									    	    paged++;
									    	    ajax_running = false;
									    	    if (length < posts_per_page) {
									    	    	all_loaded = true;
									    	    }
											   }
											   
											});//end ajax
										}//end if all loaded
									}//end if window scrolltop
								});//end scroll function

						    });

						</script>

