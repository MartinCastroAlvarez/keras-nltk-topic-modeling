							<div class="header-tripbuilder" title="Trip Planner">
								<div class="contentRender contentRender_5 contentRender_type_widget contentRender_name_plugins_tripbuilder_counter" data-sv-time="2" data-sv-index="5" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>
<div class="tripbuilder" id="tripbuilder_counter_cd119687-04bc-42e2-a7e7-cab6f61a17b0"></div>
<div class="tripbuilder-message">
	<span>You've added your first Trip Builder item! Keep track of your trip itinerary here.</span>
	<div class="tb-message-close" data-tb-message-close>
		<i class="fa fa-times"></i>
	</div>
</div>
<script>
	require(["jquery", "sv_clientLib", "site_tripbuilder"], function($, clientLib, tripbuilder) {
		if (!tripbuilder.enabled) { return; }

		var root = $("#tripbuilder_counter_cd119687-04bc-42e2-a7e7-cab6f61a17b0");
		var container = $("[data-sv-tripbuilderCounter]");
		var renderContainer = root.closest(".contentRender_name_plugins_tripbuilder_tripbuilder_counter");
		container.append(renderContainer);
		var onChangeCB = function() {
			var tb_message_displayed = clientLib.getCookie('tripbuilder-message-seen');
			if (tripbuilder.getTotalCount() > 0) {
				root.html("<a class='active' href='" + tripbuilder.path + "'><strong><i class='fa fa-heart-o'></i> <span class='counter'>" + tripbuilder.getTotalCount() + "</span></a></strong>");

				if(tb_message_displayed !== "true" && tripbuilder.getTotalCount() == 1) {
					clientLib.setCookie({ name : 'tripbuilder-message-seen', value : 'true', path : '/', 'max-age' : 86400 });
					$('.tripbuilder-message').addClass('open');
					setTimeout(function() {
						$('.tripbuilder-message').removeClass('open');
					}, 10000)
				} else {
					clientLib.setCookie({ name : 'tripbuilder-message-seen', value : 'true', path : '/', 'max-age' : 86400 });
					$('.tripbuilder-message').removeClass('open');
				}
				
			} else {
				root.html("<a href='" + tripbuilder.path + "'><i class='fa fa-heart-o'></i></a>");
				$('.tripbuilder-message').removeClass('open');
				clientLib.setCookie({ name : 'tripbuilder-message-seen', value : 'false', path : '/', 'max-age' : 86400 });
			}
		}
		onChangeCB();
		tripbuilder.events.on("change", onChangeCB);

		$('[data-tb-message-close]').on('click', function() {
			clientLib.setCookie({ name : 'tripbuilder-message-seen', value : 'true', path : '/', 'max-age' : 86400 });
			$('.tripbuilder-message').removeClass('open');
		});
	});
</script>
</div>
							</div>
						
						
