		<div class="contentRender contentRender_16 contentRender_type_widget contentRender_name_plugins_events_detail" data-sv-time="26" data-sv-index="16" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>










<div id="eventDetails_8d5765a3-2620-430f-b407-8ae9274a2eea" class="listings" data-type="event" data-recid="3211" data-sv-detail-page data-gtm-vars='{"tClient": {"eventCategory": "Events", "eventLabel": "Oak%20Street | Oak%20Street%20Po-Boy%20Festival", "et": "Oak%20Street%20Po-Boy%20Festival", "vt": "Detail", "ua": "Click"}}'>
	<script type="text/template" data-sv-infoWindowTemplate><div class="infoWindow myDiv">
	{{!listings}}
		<h5>{{title}}</h5>
		<ul class="contactInfo no-bullet">
			{{:address1}}
				<li>{{address1}}</li>
			{{/}}
			{{:city}}
				<li>{{~plugins.stringLib.formatCityStateZip({ city : data.city, state : data.state, zip : data.zip })}}</li>
