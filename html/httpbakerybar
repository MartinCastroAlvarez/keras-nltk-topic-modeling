<script>
  function applyActiveStyle(inputId) {
    if (document.getElementById('menu-select-' + inputId).checked) {
      var labels = document.querySelectorAll('.menu-select-labels');
      for (var i=0; i < labels.length; i++) {
        labels[i].classList.remove('menu-select-labels--active');
      }
      document.getElementById('menu-select-label-' + inputId).classList.add('menu-select-labels--active');
    }
  }
</script>
