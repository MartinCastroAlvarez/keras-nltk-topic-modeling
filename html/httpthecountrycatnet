      <div class="title-desc-inner" data-collection-id="5214416ee4b0f809aa867d66" data-edit-main-image="Background">
        <div class="page-title-wrapper"><h1 class="page-title" data-content-field="title">The Country Cat</h1></div>
        <br> <!-- this keeps the inline-blocks from collapsing -->
        <div class="page-desc" data-content-field="description"></div>
      </div>
    <div class="scroll-arrow"><span>SCROLL DOWN</span></div>
