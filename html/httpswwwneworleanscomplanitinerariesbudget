<script>
	require(["jquery"], function() {
		// This should exist in the header panel and adds the body class as quickly as possible to hide
		// all the items on the page so we don't have the site flash before the calendar shows up
		if (location.search.indexOf('action=popupCal') > -1 && document.body.querySelectorAll('.lookup-calHolder')) {
			document.body.classList.add('crm-calendar-popup');
		};
	})
</script>
</div>

	<section class="header-slideshow">
		<div class="contentRender contentRender_15 contentRender_type_widget contentRender_name_plugins_collections_template_custom_header_slideshow_interior" data-sv-time="80" data-sv-index="15" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>



<section class="header-slideshow-int noscript custom_header_slideshow_blog" data-sv-header-slideshow data-guid="dd0f21e7-b102-40e6-b404-43d44aa9e217">
    <div class="int-header">
    	<script>document.querySelector('[data-guid="dd0f21e7-b102-40e6-b404-43d44aa9e217"]').classList.add('hidden');</script>
		<script type="text/template" data-sv-slide-template>
	{{#items}}
		{{:asset}}
			<div class="item slide {{!@first}}lazy{{/}}">
				{{:~equal(data.asset.type, "video")}}
					{{~setvar('sliderHasVideos', true)}}
					{{~setVar("videourl", helpers.exec(function() {
		        		if (data.asset.videoid !== undefined){
		          			if(data.asset.videotype === "vimeo") {
		            			return 'https://player.vimeo.com/video/' + data.asset.videoid;
							} else {
		            			return 'https://youtube.com/watch?v=' + data.asset.videoid;
		        			}
		        		}
		        		return false;
		      		}))}}
					<a class="open-popup-link" href="{{~var.videourl}}" target="_blank" data-sv-play>
						<div class="play-btn">
