define("ta/servlet/Popup", ['ns-exporter', 'lib/jquery-amd', 'ta/Browser', 'ta/util/Error', 'ta/Core/TA.Record', 'ta/Core/TA.Store', 'ta/util/Cookie', "api-mod"],
function(nsExporter, $, taBrowser, taError, taRecord, taStore, taCookie, api) {
var _index=0;
function _preventDefault(event) {
if (event) {
if (event.preventDefault) {
event.preventDefault();
} else {
event.returnValue = false;
}
}
}
var exports = {
open: function(url, name, width, height, x, y, noScroll, allOptions)
{
try {
if(isNaN(parseInt(width))) {
width = 600;
}
else {
width = parseInt(width);
}
if(isNaN(parseInt(height))) {
height = 400;
}
else {
height = parseInt(height);
}
if(isNaN(parseInt(x))) {
x = 0;
}
else {
x = parseInt(x);
}
if(isNaN(parseInt(y))) {
y = 0;
}
else {
y = parseInt(y);
}
if (url.indexOf("p=HotelsCom") > -1) {
width = Math.max(width, 960);
height = Math.max(
height,
window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight,
600);
}
if (taBrowser.isChrome()) {
noScroll = true;
}
var noScrollVal = 1;
if(noScroll) {
noScrollVal = 0;
}
var menubar = 1;
if ( taBrowser.isIE9orHigher() )
{
var menubar = 0;
}
var winName = false;
if (name && typeof name == "string") {
winName = name;
}
if (!winName) {
var popNum = _index++;
winName = "TripPop" + popNum;
}
var ops = "";
if (allOptions) {
ops = "toolbar=1,resizable=1,location=1,status=1";
}
else {
menubar = 0;
ops = "toolbar=0,resizable=1,location=0,status=0";
}
ops += ",menubar="+menubar;
ops += ",scrollbars="+noScrollVal;
ops += ",width="+width;
ops += ",height="+height;
ops += ",screenX="+x+",left="+x;
ops += ",screenY="+y+",top="+y;
var w = window.open(url, winName, ops);
if (w != null) {
try {
w.focus();
w.opener = self;
} catch (exc) {}
return w;
}
} catch(e) {
taError.record(e, 'Popup.open', null, { name: name, width: width, height: height, x: x, y: y, noScroll: noScroll, allOptions: allOptions});
}
},
openExternalWithDefaultOptions: function(url) {
try {
var width = 475;
var height = 390;
var x = 210;
var y = 5;
if ( screen.width >= 1024 && screen.height >= 768) {
width = 900;
height = 768;
x = 240;
}
else if (screen.width > 1024) {
width = 800;
height = 600;
x = 240;
}
else if (screen.width > 800) {
width = 620;
height = 500;
x = 240;
}
if (url.indexOf("p=HotelsCom") > -1) {
width = Math.max(width, 960);
height = Math.max(
height,
window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight,
600);
}
var noScroll = false;
if (taBrowser.isChrome()) {
noScroll = true;
}
var noScrollVal = 1;
if(noScroll) {
noScrollVal = 0;
}
var menubar = 1;
if ( taBrowser.isIE9orHigher() )
{
var menubar = 0;
}
var popNum = _index++;
var winName = "TripPop" + popNum;
var ops = "toolbar=1,resizable=1,location=1,status=1";
ops += ",menubar="+menubar;
ops += ",scrollbars="+noScrollVal;
ops += ",width="+width;
ops += ",height="+height;
ops += ",screenX="+x+",left="+x;
ops += ",screenY="+y+",top="+y;
var w = window.open(url, winName, ops);
if (w != null) {
try {
w.focus();
w.opener = self;
} catch (exc) {}
return w;
}
} catch(e) {
taError.record(e, 'Popup.open - openExternalWithDefaultOptions', null, { name: name, width: width, height: height, x: x, y: y, noScroll: noScroll});
}
},
openSizedPopup: function(evnt, elmt, noScroll)
{
_preventDefault(evnt);
var width = 600;
var height = 400;
if (sz = elmt.className.match(/sz(\d+)x(\d+)/)) {
width = sz[1];
height = sz[2];
}
exports.open(elmt.href, 'photoTPW', width, height, false, false, ( (typeof noScroll !== 'undefined')? noScroll: true), true);
},
checkrates: function(evnt, elmt) {
_preventDefault(evnt);
elmt=$(elmt).closest("a")[0];
exports.open(elmt.href, 'cr', 245, 610, 5, 5);
taRecord.trackEventOnPage('map_pin_overlay', 'check_rates_click');
},
sponsorCheckRates: function(evnt, elmt) {
_preventDefault(evnt);
elmt=$(elmt).closest("span")[0];
var accomId = elmt.className.match(/id_(\d+)/)[1];
var srcUrl = "/HotelDateSearch?d=" + accomId + "&fromPop=false&sponsid=" + taStore.retrieve('sponsor.id') + '&area=QC_Button_Map&from=HACSearchButton&returnTo=__2F__GMapsLocationController&sponsdet=mapsHacBottom';
exports.open(srcUrl, 'cr', 245, 610, 5, 5);
},
termsPopup: function(event, elmt) {
popup.apply(elmt, [event, 'terms', 300, 300, 30, 25]);
return false;
},
openInParent: function(event, elmt, leaveOpen) {
var w = false;
if (window.top && window.top.opener) w = window.top;
else if (window.opener) w = window;
if (!w) return true;
var url = '';
if(elmt.nodeName === 'FORM') {
url = elmt.action + '?' + api.toFormQueryString(elmt);
}
else {
elmt=$(elmt).closest("a")[0];
if (elmt) {
url = elmt.href;
}
}
if(url !== '') {
_preventDefault(event);
w.opener.location = url;
if (! (typeof leaveOpen !== 'undefined' && leaveOpen) ) {
w.close();
} else {
window.blur();
}
}
},
openInParentOrSelf: function(event, elmt) {
var w = false;
if (window.top && window.top.opener) w = window.top;
else if (window.opener) w = window;
var url = '';
if(elmt.nodeName === 'FORM') {
url = elmt.action + '?' + api.toFormQueryString(elmt);
}
else {
elmt=$(elmt).closest("a")[0];
if (elmt) {
url = elmt.href;
}
}
if(url !== '') {
_preventDefault(event);
if (!w || (typeof w.opener == 'undefined') || !w.opener || (typeof w.opener.closed === 'undefined') || w.opener.closed)
{
window.location = url;
}
else
{
w.opener.location = url;
w.close();
}
}
},
photoGallery: function(event, elmt) {
elmt=$(elmt).closest("a")[0];
if (!elmt) {
taError.record(e, 'photoGallery - get anchor elmt');
return false;
}
var url = "";
var id = "1";
if(elmt.className)
{
url = elmt.className.match(/u_([^ ]*)/)[1];
id = elmt.className.match(/#([0-9]+)/)[1];
}
try{
if(typeof cookieDomain == "undefined")
{
var h = window.location.hostname.split('.'), i=0, dd;
while(h[i] && h[i] != "tripadvisor") {
i++;
}
dd = h.slice(i).join('.');
if(dd) {
cookieDomain = "." + dd;
}
}
taCookie.write("PhotoPop", id, null, null, cookieDomain);
}
catch(e) {
taError.record(e, 'photoGallery - cookie set');
}
try {
var w = "";
if(url)
{
w = exports.open(url, 'media', 780, 800);
}
else
{
}
try{
if (w && w.showImage) {
w.showImage(id);
}
}
catch(e) { }
}
catch(e) {
taError.record(e, 'photoGallery - pop window');
}
return false;
},
openAIMLink:function(event,elmt)
{
_preventDefault(event);
var url;
var srcel = $(elmt).closest("span.fkASDF,div.fkASDF,a")[0];
if (!srcel) {
return;
} else if (srcel.nodeName === 'A') {
url = srcel.href;
} else {
url = elmt.className.match(/u_([^ ]*)/)[1];
}
var navarea = "navarea";
if (ta.meta && ta.meta.link && ta.meta.link.getPhotoNavArea) {
navarea = ta.meta.link.getPhotoNavArea(elmt);
}
url = url.replace("%7Cnavarea%7C", navarea);
var winName = elmt.className.match(/win(.+)/);
if (winName && winName.length>1) {
winName = winName[1];
} else {
winName = null;
}
taStore.store('commerce.AIMWindow', exports.open(url,winName,980,800,240,5,false,true));
},
openProviderWindow: function(evnt, elmt)
{
_preventDefault(evnt);
exports.open(elmt.href, 'photoTPW', 1260, 922, false, false, true, true);
}
};
nsExporter.exportTo(exports,"ta.servlet.Popup");
return exports;
});

define('ta/util/ASDF', ['vanillajs', 'utils/asdf-encoder', 'ta/util/Cookie', 'ns-exporter', 'ta/servlet/Popup'],
function(vanilla, Encoder, cookie, nsExporter, taPopup) {
var exports= {
asdf: function(str, pid) {
if (pid) {
cookie.setPIDCookie(pid);
}
window.location = Encoder.asdf(str);
},
asdfPopup: function(str, pid, name, width, height) {
if (pid) {
cookie.setPIDCookie(pid);
}
taPopup.open(Encoder.asdf(str), name, width, height);
},
asdfPopupTerms: function(str) {
taPopup.open(Encoder.asdf(str),'terms',300,300,30,25);
},
asdfDcd: function(str) {
return Encoder.asdf(str);
}
};
nsExporter.exportTo(exports, "ta.util.ASDF");
return exports;
});
