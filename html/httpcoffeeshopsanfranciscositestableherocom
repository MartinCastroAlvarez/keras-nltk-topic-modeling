            </script>
            <script>
            /*Google Maps*/
            function initMap() {
                var restaurantLocation = new google.maps.LatLng(37.7470855, -122.4187899),
                    gMap = new google.maps.Map(document.getElementById('map'), {
                        center: restaurantLocation,
                        zoom: 18,
                        scrollwheel: false,
                        zoomControl: false,
                        mapTypeControl: false,
                        zoomControlOptions: {
                            position: google.maps.ControlPosition.RIGHT_CENTER
                        }
                    }),
                    marker = new google.maps.Marker({
                        position: restaurantLocation,
                        map: gMap,
                        icon: '/images/common/icon-map-cafe.svg'
                    });
            }
            </script>
