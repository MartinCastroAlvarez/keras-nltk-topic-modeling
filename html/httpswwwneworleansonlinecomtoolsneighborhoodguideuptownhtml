    <td height="213" style="background-image:url(/images/main/prefooterBG.jpg)"><table border="0" align="center" cellpadding="0" cellspacing="0">
      <tr>
        <td align="center" valign="top"><a href="/tools/gallery/"><img src="/images/main/prefooterGallery.png" width="195" height="163" /></a><br />
          <a href="/tools/gallery/" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('viewGallery','','/images/main/viewGalleryRoll.png',1)"><img src="/images/main/viewGallery.png" alt="View Gallery" width="89" height="18" id="viewGallery" /></a></td>
        <td align="center" width="20px"><img src="/images/main/prefooterDivider.gif" width="1" height="165" /></td>
        <td align="center" valign="middle"><a href="/calendar/index.html"><img src="/images/main/prefooterCalendarText.png" width="128" height="167" /></a><br>
 <a href="/calendar/index.html" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('seeMore','','/images/main/seeMoreRoll.png',1)"><img src="/images/main/seeMore.png" alt="See More" width="65" height="18" id="seeMore" style="margin-top:4px;"></a></td>
        <td width="200" align="center" valign="top"><br />          <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr>
    <td class="calendar"><div id="cal-container"></div>
<script language="javascript" type="text/javascript">
function displayCalendar(themonth, theday, theyear) {
//alert(themonth+", "+theday+", "+theyear);
//  SET ARRAYS
var day_of_week = new Array('SUN','MON','TUE','WED','THU','FRI','SAT');
var month_of_year = new Array('January','February','March','APRIL','May','June','July','August','September','October','November','December');

//  DECLARE AND INITIALIZE VARIABLES
var Calendar = new Date();
Calendar.setFullYear(theyear,themonth,theday);
var nextMonth = new Date();
nextMonth.setFullYear(theyear,themonth,theday);
nextMonth.setMonth(Calendar.getMonth()+1);
var previousMonth = new Date();
previousMonth.setFullYear(theyear,themonth,theday);
previousMonth.setMonth(Calendar.getMonth()-1);
var todaysDate = new Date();
todaysDate.setFullYear(2019, 6, 26);
todaysDate.setHours(0,0,0,0);

var year = Calendar.getFullYear();	    // Returns year
var month = Calendar.getMonth();    // Returns month (0-11)
var month2 = Calendar.getMonth()+1;    // Returns month (1-12)
var today = Calendar.getDate();    // Returns day (1-31)
var weekday = Calendar.getDay();    // Returns day (1-31)

var DAYS_OF_WEEK = 7;    // "constant" for number of days in a week
var DAYS_OF_MONTH = 31;    // "constant" for number of days in a month
var cal;    // Used for printing

Calendar.setDate(1);    // Start the calendar day at '1'
Calendar.setMonth(month);    // Start the calendar month at now


/* VARIABLES FOR FORMATTING
NOTE: You can format the 'BORDER', 'BGCOLOR', 'CELLPADDING', 'BORDERCOLOR'
      tags to customize your caledanr's look. */

var TR_start = '<TR>';
var TR_end = '</TR>';
var highlight_start = '<TD WIDTH="31" STYLE="background-image:url(/images/main/calendarCurrentDay.png); background-position:center; background-repeat:no-repeat; height:20px; text-align:center;"><B><CENTER>';
var highlight_end   = '</CENTER></B>';
var TD_start = '<TD WIDTH="31" style="height:18px;"><CENTER>';
var TD_end = '</CENTER></TD>';

/* BEGIN CODE FOR CALENDAR
NOTE: You can format the 'BORDER', 'BGCOLOR', 'CELLPADDING', 'BORDERCOLOR'
tags to customize your calendar's look.*/

cal =  '<TABLE CELLSPACING=0 CELLPADDING=0 CLASS="Calendar" WIDTH="180" ALIGN="center"><TR><TD>';
cal += '<TABLE BORDER=0 CELLSPACING=0 CELLPADDING=1 CLASS="Calendar"><TR>';
cal += '<TD width="5" align="left">&nbsp;<a href="javascript:displayCalendar('+previousMonth.getMonth()+', '+previousMonth.getDate()+', '+previousMonth.getFullYear()+');" style="text-decoration:none;"><strong><</strong></a></TD><TD COLSPAN="' + ((DAYS_OF_WEEK)-2) + '" align="center" style="color:#FFF;"><B>';
cal += month_of_year[month]  + '   ' + year + '</B></TD><TD width="5" align="right"><a href="javascript: displayCalendar('+nextMonth.getMonth()+', '+nextMonth.getDate()+', '+nextMonth.getFullYear()+');" style="text-decoration:none;"><strong>></strong></a>&nbsp;</TD>' + TR_end;
cal += TR_start;

//   DO NOT EDIT BELOW THIS POINT  //

// LOOPS FOR EACH DAY OF WEEK
for(index=0; index < DAYS_OF_WEEK; index++)
{

// PRINTS DAY
cal += TD_start + '<div style="font-size:8px; color:#FFF; font-weight:bold;">' + day_of_week[index] + '</div>' + TD_end;
}

cal += TD_end + TR_end;
cal += TR_start;

// FILL IN BLANK GAPS UNTIL TODAY'S DAY
for(index=0; index < Calendar.getDay(); index++)
cal += TD_start + '  ' + TD_end;

// LOOPS FOR EACH DAY IN CALENDAR
for(index=0; index < DAYS_OF_MONTH; index++)
{
if( Calendar.getDate() > index )
{
  // RETURNS THE NEXT DAY TO PRINT
  week_day =Calendar.getDay();

  // START NEW ROW FOR FIRST DAY OF WEEK
  if(week_day == 0)
  cal += TR_start;

  if(week_day != DAYS_OF_WEEK)
  {

  // SET VARIABLE INSIDE LOOP FOR INCREMENTING PURPOSES
  var day  = Calendar.getDate();

  // HIGHLIGHT TODAY'S DATE
  var thisDate = new Date();
  thisDate.setFullYear(year,month,day);
  thisDate.setHours(0,0,0,0);
  //alert(thisDate+"\n"+todaysDate);
  if (thisDate < todaysDate) 
  //alert("before");
  cal += TD_start + '<div style="color:#CCC; font-size:10px;"><del>' + day + '</del></div>' + TD_end;
  else if (thisDate > todaysDate) 
  //alert("after");
    //cal += TD_start + '<A href="/calendar/events.html?startdate=' + year + '-' + month2 + '-' + day + '" style="color: #FFF; font-weight:normal; font-size:10px;">' + day + '</A>' + TD_end;
	cal += TD_start + '<div style="color:#FFF; font-size:10px;">' + day + '</div>' + TD_end;
  else
  //alert("today");
  //cal += highlight_start + '<a href="/calendar/events.html?startdate=' + year + '-' + month2 + '-' + day + '" style="color: #FFF; font-weight:normal; font-size:10px;">' + day + '</a>' + highlight_end + TD_end;
  cal += highlight_start + '<div style="color:#FFF; font-size:10px;">' + day + '</div>' + highlight_end + TD_end;
  }

  // END ROW FOR LAST DAY OF WEEK
  if(week_day == DAYS_OF_WEEK)
  cal += TR_end;
  }

  // INCREMENTS UNTIL END OF THE MONTH
  Calendar.setDate(Calendar.getDate()+1);

}// end for loop

cal += '</TD></TR></TABLE></TABLE>';

//  PRINT CALENDAR
document.getElementById('cal-container').innerHTML = cal;
//document.write(cal);
}
</script></td>
  </tr>
</table>
