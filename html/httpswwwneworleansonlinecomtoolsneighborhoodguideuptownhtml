<script type="text/javascript">
		
        function mecValidateFormS2(trt) {
        	var isMobile = false;
        	if((/iPhone|iPod|iPad|Android|BlackBerry/).test(navigator.userAgent)) {
        		var isMobile = true;
        	}
			if(isMobile){
        		var emailText =  $('#mecFrame1 .newsletter_email'); var mErr =  $('#mecFrame1 .popError'); var subField =  $('#mecFrame1 .newsSub'); var sForm =  $('#mecFrame1 form'); var tThanks = $('.mbl_Thanks');var tOffer = $('.mbl_Offer');
            }else{
        		var emailText =  $('#m_pop .newsletter_email'); var mErr =  $('#m_pop .popError'); var subField =  $('#m_pop .newsSub'); var sForm =  $('#m_pop form'); var tThanks = $('.m_Thanks'); var tOffer = $('.m_Offer');
            }
            var regExp = new RegExp(/^\S+@\S+\.\S+$/);
            if ((emailText.val() == '') || emailText.val() == 'John.Smith@gmail.com' || (!regExp.test(emailText.val()))) {
                $('.popError').css('display', 'block');
                return false;
            } else {
                $('.popError').css('display', 'none');               
                closeCookie();
                var newsletterSub = function(){
                    if(subField.val() == 1){
                    subField.val(2); 
                    $.post('/tools/signup_done.html', sForm.serialize());
				
                     }
                }
                if(subField.val() == 2){
                    setTimeout(function(){ subField.val(1);}, 700);
                   return false;
                }else{
                    subField.val(1);
                    newsletterSub(subField.val());
                    setTimeout(function() {  
                    if($('.mBody')){
                    	$('.mBody').css('height','0%'); 
                    }                     
                    tOffer.css('display','none');
                    tThanks.css('display','block');
                    if(tThanks.is(':visible')){
                    	conversionNewsletterGA(4);
                    }
                    }, 1250);

                    // MECLABS - prevent slideout from opening, close static
                    if ($(".newsletter_slideout").size() > 0) {
                        $(".newsletter_slideout").addClass("closed");
                        $("#slideout_active").val("0");
                    }
                    if ($(".static_header").size() > 0) {
                        $(".static_header").hide();
                        $("#static_active").val("0");
                    }
                    var cookie_expire = getCookieExpireDate(730); // two years out
                    document.cookie = "convertedNewsletter=1; expires="+cookie_expire+"; path=/";
                    return false;
                }   
            }
        }

        // validation and submission of the newsletter via the slideout/static
        function mecNewsletterValidation(section) {
            var el;
            switch (section) {
                case 'slideout': el = '.newsletter_slideout_content'; break;
                case 'static': el = '.static_header_form'; break;
            }
            var emailText =  $(el+' input[name="ea"]');
            var mErr =  $(el+' .emError');
            var subField =  $(el+' .newsSub');
            var sForm =  $(el+' form');

            var regExp = new RegExp(/^\S+@\S+\.\S+$/);
            if ((emailText.val() == '') || emailText.val() == 'John.Smith@gmail.com' || (!regExp.test(emailText.val()))) {
                $(el+' form').find('.emError').show();
                return false;
            } else {
                $(el+' form').find('.emError').hide();
                if (subField.val() == 1) {
                    subField.val(2);
                    $.post('/tools/signup_done.html', sForm.serialize(), function(){

                        // close popup cookie and set converted newsletter cookie
                        var cookie_expire = getCookieExpireDate(730); // 2 years out
                        document.cookie = "closePop=1; expires="+cookie_expire+"; path=/";
                        document.cookie = "convertedNewsletter=1; expires="+cookie_expire+"; path=/";

                        // show thanks msg, fire GA event, and close asset if needed
                        if (section == 'slideout') {
                            $(".newsletter_slideout").addClass("closed");
                            setTimeout(function(){
                                $("#newsSignSlideout").hide();
                                $(".slideout_copy_head").html("THANK YOU FOR SIGNING UP!");
                                $(".slideout_copy_sub").css("width","95%");
                                $(".slideout_copy_sub").css("line-height","17px");
                                $(".slideout_copy_sub").html("You are now subscribed to our New Orleans e-newsletter and we'll send you the news each month.");
                                ga("send", {hitType: "event", eventCategory: "Signup Form", eventAction: "NewOrleansOnline Monthly E-newsletter", eventLabel: "slideout_Conversions",  nonInteraction: true});
                                $("#static_header").hide();
                                subField.val(1);
                            }, 500);
                            setTimeout(function(){
                                $(".newsletter_slideout").animate({right: -320},500,'',function(){});
                            },5000);
                        } else if (section == 'static') {
                            subField.val(1);
                            ga("send", {hitType: "event", eventCategory: "Signup Form", eventAction: "NewOrleansOnline Monthly E-newsletter", eventLabel: "static_Conversions",  nonInteraction: true});
                            setTimeout(function(){ location.href = '/tools/signup_done.html'; }, 500);
                        }
                    });
                }
            }
        }

        $(document).ready(function() {   
		    $(document).bind('keypress', function(e) {	       
		        if (e.keyCode == 13) {
		            e.preventDefault();
		            if ($('#eaText').is(':focus'))
		                $('.sgnCta').trigger('click');

                    if ($('#slideoutNewsletterEmail').is(':focus'))
                        $('.slideout_newsletter_cta').trigger('click');

                    if ($('#staticNewsletterEmail').is(':focus'))
                        $('.static_newsletter_cta').trigger('click');

                    // trigger search submit
                    if ($('form#searchbox_000581986597510267301\\:1awrvx66qgw').find('input[type="text"]').is(':focus')) {
                        $("#searchbox_000581986597510267301\\:1awrvx66qgw").submit();
                    }
		        }
		    });
        });

</script>

