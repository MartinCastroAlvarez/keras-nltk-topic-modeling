<script language="javascript">
  function trim(s){
    return s.replace(/^\s*(\S*(\s+\S+)*)\s*$/, "$1");
  }

  function MLABSValidateForm() {
    if($('.newsLttr').hasClass('hideN')){
      setTimeout(function() {document.MLABS_signup.submit();},1250);
    }
    if(!$('.newsLttr').hasClass('hideN')) {
    var errorMsg = '';
    if ((document.MLABS_signup.name_first.value) == '' || document.MLABS_signup.name_first.value == 'First Name') {
      errorMsg+="You must provide your first name\r\n";
      document.getElementById("fnError").style.display = 'inline';
    } else {
      document.getElementById("fnError").style.display = 'none';
    }
    if (document.MLABS_signup.name_last.value == '' || document.MLABS_signup.name_last.value == 'Last Name') {
      errorMsg+="You must provide your last name\r\n";
      document.getElementById("lnError").style.display = 'inline';
    } else {
      document.getElementById("lnError").style.display = 'none';
    }
    var regExp = new RegExp(/^\S+@\S+\.\S+$/);
        if ((document.MLABS_signup.ea.value == '') || document.MLABS_signup.ea.value == 'john.doe@email.com' || (!regExp.test(document.MLABS_signup.ea.value))) {
      errorMsg+="You must provide your valid email address\r\n";
      document.getElementById("emailError").style.display = 'inline';
    } else {
      document.getElementById("emailError").style.display = 'none';
    }
    
    if (errorMsg != '') {
      if (document.getElementById) {
        document.getElementById("error").style.display = 'inline';
      } else {
        alert(errorMsg);
      }
    } else {
      document.getElementById("error").style.display = 'none';
      // 2014.03.26 MecLabs

      if (document.getElementById("newsletters").checked) {
        ga('send','event', 'Signup Form', 'NewOrleansOnline Monthly E-newsletter','Overlay');
      }
      // End MecLabs
              setTimeout(function() {document.MLABS_signup.submit();},1250);
          }
  }
  } // end MLABSValidateForm()
  //Add in default input text
</script>
</td>
          </tr>
        </table></td>
    </tr>
  </table>
