                        $('#divSatisfiChat #iframeContainer').attr('style',Style);
                        IsZIndexSet=true;
                    }


              },
                OnHideApp: function () {
                    //this will be fired after the app is hidden.


              },
                OnResize: function () {
                    // this will be fired after resize of pop up.
                }
            };
            var LaunchSetting = {
                HideDefaultLaunch: false,
                ButtonSelector: ".open_popup"
            }
