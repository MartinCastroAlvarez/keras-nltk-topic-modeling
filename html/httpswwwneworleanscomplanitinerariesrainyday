<script>
	require(["jquery"], function() {
		// This should exist in the header panel and adds the body class as quickly as possible to hide
		// all the items on the page so we don't have the site flash before the calendar shows up
		if (location.search.indexOf('action=popupCal') > -1 && document.body.querySelectorAll('.lookup-calHolder')) {
			document.body.classList.add('crm-calendar-popup');
		};
	})
</script>
</div>

	<section class="header-slideshow">
		<div class="contentRender contentRender_15 contentRender_type_widget contentRender_name_plugins_collections_template_custom_header_slideshow_interior" data-sv-time="39" data-sv-index="15" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>



<section class="header-slideshow-int noscript custom_header_slideshow_blog" data-sv-header-slideshow data-guid="ec0772bb-8e92-48f9-8817-43810bd0338f">
    <div class="int-header">
    	<script>document.querySelector('[data-guid="ec0772bb-8e92-48f9-8817-43810bd0338f"]').classList.add('hidden');</script>
		<script type="text/template" data-sv-slide-template>
	{{#items}}
		{{:asset}}
			<div class="item slide {{!@first}}lazy{{/}}">
				{{:~equal(data.asset.type, "video")}}
					{{~setvar('sliderHasVideos', true)}}
					{{~setVar("videourl", helpers.exec(function() {
		        		if (data.asset.videoid !== undefined){
		          			if(data.asset.videotype === "vimeo") {
		            			return 'https://player.vimeo.com/video/' + data.asset.videoid;
							} else {
		            			return 'https://youtube.com/watch?v=' + data.asset.videoid;
		        			}
		        		}
		        		return false;
		      		}))}}
					<a class="open-popup-link" href="{{~var.videourl}}" target="_blank" data-sv-play>
						<div class="play-btn">
						  <img src="/includes/public/assets/images/icons/play-icon.png">
						</div>
		    	{{/}}

			    	{{~setVar('resource', helpers.plugins.cloudinary.createResource({ raw : data.asset.resource_raw }))}}

			    	{{:asset_mobile}}
			          {{~setVar('resource_mobile', helpers.plugins.cloudinary.createResource({ raw : data.asset_mobile.resource_raw }))}}
			        {{/}}

			        {{:asset_mobile}}
			        	<div class="img-cont hide-for-medium-up">
			                <img src="{{~var.resource_mobile.getThumbnailUrl({ height : 800, width : 640, crop : 'fill'})}}" alt="{{%asset_mobile.alt_text}}"/>
			            </div>
			        {{?}}
			        	<div class="img-cont hide-for-medium-up">
			                <img src="{{~var.resource.getThumbnailUrl({ height : 800, width : 640, crop : 'fill' })}}" alt="{{%asset.alt_text}}"/>
			            </div>
			        {{/}}
		            <div class="img-cont show-for-medium-up {{:show_underneath}}show-underneath{{/}}">
		                <img src="{{~var.resource.getThumbnailUrl({ height : 500, width : 1400, crop : 'fill'})}}" alt="{{%asset.alt_text}}"/>
					</div>
					{{:show_underneath}}
						<div class="text-cont-underneath">
							<div class="row">
								{{:show_title}}

									<div class="title">
										{{:link}}<a href="{{link.url}}">{{/link}}
